<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:telerik="http://schemas.telerik.com/2022/xaml/maui"
             x:Class="SDKBrowserMaui.Examples.AIPromptControl.GettingStartedCategory.GettingStartedExample.AIPromptGettingStartedXaml">
    <Grid>
        <!-- >> aiprompt-getting-started-xaml -->
        <telerik:RadAIPrompt x:Name="aiPrompt"
                             InputText="{Binding InputText}"
                             PromptRequestCommand="{Binding PromptRequestCommand}"
                             OutputItems="{Binding OutputItems}" />
        <!-- << aiprompt-getting-started-xaml -->
    </Grid>
</ContentView>
