<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:telerik="http://schemas.telerik.com/2022/xaml/maui"
             x:Class="SDKBrowserMaui.Examples.SchedulerControl.GettingStartedCategory.GettingStartedExample.GettingStartedXaml">
    <Grid>
        <!-- >> scheduler-getting-started-xaml -->
        <telerik:RadScheduler AutomationId="scheduler">
            <telerik:RadScheduler.ViewDefinitions>
                <telerik:WeekViewDefinition />
                <telerik:WeekViewDefinition IsWeekendVisible="False" Title="Work Week" />
                <telerik:MultidayViewDefinition VisibleDays="3" Title="3 Days" />
                <telerik:DayViewDefinition />
                <telerik:MonthViewDefinition />
            </telerik:RadScheduler.ViewDefinitions>
        </telerik:RadScheduler>
        <!-- << scheduler-getting-started-xaml -->
    </Grid>
</ContentView>
